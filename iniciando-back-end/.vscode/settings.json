{
    "git.ignoreLimitWarning": true,

    "[javascript]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true,
        }
    },
    "[javascriptreact]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true,
        }
    },
    "[typescript]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true,
        }
    },
    "[typescriptreact]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true,
        }
    },
    "editor.formatOnSave": false,
        "elint.validate":[
            "html",
            "javascript",
            "typescript",
            "javascriptreact",
            "typescriptreact"
        ],
    "eslint.format.enable": true,

}
